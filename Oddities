#include <stdio.h>

int main()
{
    int nums, num;
    scanf("%d", &nums);
    for(int i = 0; i < nums; i++){
        scanf("%d", &num);
        if(num % 2 == 0){
            printf("%d is even\n", num);
        }else if (num % 2 != 0){
            printf("%d is odd\n", num);
        }
    }
    return 0;
}
